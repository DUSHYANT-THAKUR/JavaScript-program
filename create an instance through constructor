        <title>Dushyant</title>
    </head>
    <body>
        <p id="demo">hello</p>
        <script>
           function myfunction(id,name,salary)
           {
               this.id=id;
               this.name=name;
               this.salary=salary;
           }
           emp=new myfunction(10,"dushyant",1000);
            document.getElementById("demo").innerHTML=emp.id+" "+emp.name+" "+emp.salary;
        </script>
    </body>
